{
    "name": "aiida-fireengine",
    "author": "Bonan Zhu",
    "author_email": "zhubonan@outlook.com",
    "description": "AiiDA plugin to allow using `fireworks` as the executation engine for `CalcJob`.",
    "url": "https://github.com/zhubonan/aiida-fireengine",
    "license": "MIT",
    "classifiers": [
        "Programming Language :: Python",
        "Intended Audience :: Science/Research",
        "License :: OSI Approved :: MIT License",
        "Natural Language :: English",
        "Framework :: AiiDA"
    ],
    "version": "0.1.0a0",
    "entry_points": {
        "aiida.schedulers": [
            "fireworks = aiida_fireengine.fwscheduler:FwScheduler"
        ],
        "console_scripts": [
            "arlaunch = aiida_fireengine.scripts.arlaunch_run:arlaunch"
        ]
    },
    "include_package_data": true,
    "setup_requires": ["reentry"],
    "reentry_register": true,
    "install_requires": [
        "six",
        "voluptuous",
        "fireworks==1.9.5"
    ],
    "extras_require": {
        "local": [
            "aiida-core>=1.3.0,<2.0.0"
        ],
        "testing": [
            "pgtest~=1.3.1",
            "wheel~=0.31",
            "coverage",
            "pytest~=3.6,<5.0.0",
            "pytest-cov",
            "aiida-core>=1.3.0,<2.0.0"
        ],
        "pre-commit": [
            "astroid==2.2.5",
            "pre-commit==1.20.0",
            "prospector==1.1.7",
            "pylint==2.3.1",
            "aiida-core>=1.3.0,<2.0.0"
        ],
        "docs": [
            "sphinx",
            "sphinxcontrib-contentui",
            "sphinxcontrib-details-directive; python_version>='3.0'",
            "sphinx-rtd-theme",
            "aiida-core>=1.3.0,<2.0.0"
        ]
    }
}
